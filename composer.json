{
    "name": "thanos-kontos/amock",
    "description": "PHPunit mock fixtures",
    "keywords": ["fixture", "phpunit", "test", "mock"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "phpunit/phpunit": "^6.0",
        "symfony/yaml": "^3.3"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Amock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amock\\": "tests/",
            "Fixtures\\": "tests/fixtures/sampleClasses"
        }
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.4.4"
    }
}
